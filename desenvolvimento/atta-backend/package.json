{
  "name": "atta-backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc && tsc alias",
    "start": "node tsc dist/app.js",
    "dev": "nodemon --watch './src/' --exec 'ts-node -r tsconfig-paths/register src/app.ts' -e ts",
    "prepare": "cd ../../ && husky install desenvolvimento/atta-backend/.husky",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTest",
    "test:push": "npm run test --coverage"
  },
  "keywords": [],
  "author": "Carlos Gabriel da Silva Machado",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
